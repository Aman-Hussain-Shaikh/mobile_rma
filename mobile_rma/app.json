{
  "expo": {
    "name": "Corprix RMA",
    "slug": "mobile_rma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcon/playstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Coreprix-png.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideExponentNotification": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coreprix.mobile_rma",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan barcodes"
      }
    },
    "android": {
      "package": "com.coreprix.mobile_rma",
      "versionCode": 1,
      "permissions": ["CAMERA"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Coreprix-png.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "mdpi": "./assets/AppIcon/android/mipmap-mdpi/ic_launcher.png",
        "hdpi": "./assets/AppIcon/android/mipmap-hdpi/ic_launcher.png",
        "xhdpi": "./assets/AppIcon/android/mipmap-xhdpi/ic_launcher.png",
        "xxhdpi": "./assets/AppIcon/android/mipmap-xxhdpi/ic_launcher.png",
        "xxxhdpi": "./assets/AppIcon/android/mipmap-xxxhdpi/ic_launcher.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Coreprix-png.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b17bac9-dbd3-4486-a0b4-216358a2bd59"
      }
    }
  }
}
